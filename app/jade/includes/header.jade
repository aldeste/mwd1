    header.header
      input#hamburger__checkbox.switch(type='checkbox')
      label.nav--hamburger.menu-label(for='hamburger__checkbox')
        | menu
        span.hamburger
          span.hamburger-icon
      //
        list used to accessibility, info is given to user about how many
        items are in lists if screenreaders are used
      nav.nav.switch--toggle(role='navigation')
        a.nav__action(href='#')
          svg.icon--logo
            use(xlink:href='#icon-logo')
        form.nav__form(action='#')
          input.nav__input(name='search', placeholder='search', type='search', autofocus)
        a.nav__action.ficon.ficon--list(href='#') browse
        a.nav__action.ficon.ficon--heart(href='#') pay
        div.nav__action.nav__action--has-dropdown
          input#signin__checkbox.switch(type='checkbox')
          label.nav__action.nav__action--dropdown-action.ficon.ficon--user(for='signin__checkbox')
            | sign in
          form.nav__dropdown(action='#')
            label(for='username') Username
            input#username.form__input(type='text')
            label(for='password') Password
            input#password.form__input(type='password')
            button.form__cta
              | sign in
    
